$("#search_results").append(`
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Name</th>
                <th>protein</th>
                <th>fat</th>
                <th>carbohyde</th>
                <th colspan="3"></th>
            </tr>
        </thead>
        <tbody>
            <% @results.each do |meal| %>
                <tr>
                    <td><%= meal.name %></td>
                    <td><%= meal.protein %></td>
                    <td><%= meal.fat %></td>
                    <td><%= meal.carbon %></td>
                    <td><%= link_to 'Show', meal %></td>
                    <td><%= link_to 'Edit', edit_meal_path(meal) %></td>
                    <td><%= link_to 'Destroy', meal, method: :delete, data: {confirm: "本当に削除しますか？"} %></td>
                </tr>
            <% end %>
        </tbody>
    </table>`);

