$("li.matched-food").remove()
$.each($("table#nutrition-facts tbody tr td.col-2"), function(i, val) {
    var updatedNutritionScore = $(val).text().replace(/[0-9]|\./g, "");
    $(val).text(updatedNutritionScore);
});

if (<%= @results.length %> != 0 ) {
    $match = $('<%= j(render 'search_result', results: @results) %>');
    $("ul#matching").append($match);
}

